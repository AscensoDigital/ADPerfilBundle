{
    "name": "ascensodigital/perfil-bundle",
    "type": "symfony-bundle",
    "description": "Symfony ADPerfilBundle",
    "keywords": ["Perfil","Navegacion","Reporte"],
    "homepage": "http://ascensodigital.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Corvalan",
            "email": "corvalanclaudio7@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5|~7.1",
        "symfony/symfony":"~2.7|~3.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "doctrine/orm": "~2.2,>=2.2.3|~2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "ascensodigital/component-bundle": "^1.2",
        "cocur/slugify": "^2.0",
        "javiereguiluz/easyadmin-bundle": "^1.5",
        "ext-json": "*",
        "symfony/form": "^2.8"
    },
    "autoload": {
        "psr-4": { "AscensoDigital\\PerfilBundle\\": "" }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "phpunit/phpunit": "^7.5",
        "symfony/validator": "^2.1",
        "doctrine/data-fixtures": "^1.3",
      "ext-pdo": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "AscensoDigital\\PerfilBundle\\": "./",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test:coverage": "./vendor/bin/phpunit --coverage-html tests/app/logs/coverage-report"
    },
    "version": "1.6.0"
}
