{
    "name": "ascensodigital/perfil-bundle",
    "type": "symfony-bundle",
    "description": "Symfony ADPerfilBundle",
    "keywords": ["Perfil","Navegacion","Reporte"],
    "homepage": "http://ascensodigital.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Corvalan",
            "email": "corvalanclaudio7@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "easycorp/easyadmin-bundle": "^1.17",
        "cocur/slugify": "^3.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "ascensodigital/component-bundle": "^2.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.6"
    },
    "autoload": {
        "psr-4": { "AscensoDigital\\PerfilBundle\\": "" },
        "exclude-from-classmap": [
